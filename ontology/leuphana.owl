        <rdfs:label xml:lang="en">Visiting Scientist</rdfs:label>
        <rdfs:label xml:lang="de">Gastwissenschaftler</rdfs:label>
        <rdfs:comment xml:lang="en">A visiting scientist or researcher</rdfs:comment>
    </owl:Class>
    
    <!-- Administrative Staff -->
    <owl:Class rdf:about="http://leuphana.de/ontology#AdministrativeStaff">
