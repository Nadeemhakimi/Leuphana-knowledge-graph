<?xml version="1.0"?>
<rdf:RDF xmlns="http://leuphana.de/ontology#"
     xml:base="http://leuphana.de/ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vivo="http://vivoweb.org/ontology/core#"
     xmlns:schema="http://schema.org/"
     xmlns:leuph="http://leuphana.de/ontology#">
    
    <owl:Ontology rdf:about="http://leuphana.de/ontology">
        <dc:title xml:lang="en">Leuphana University Knowledge Graph Ontology</dc:title>
        <dc:description xml:lang="en">An ontology representing the organizational and academic structure of Leuphana University of Lüneburg</dc:description>
        <dc:creator>Bachelor's Thesis Project</dc:creator>
        <dc:date>2025</dc:date>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // External Vocabulary Imports
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->
    
    <!-- FOAF imports for Person -->
    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person"/>
    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
    
    <!-- Schema.org imports -->
    <owl:Class rdf:about="http://schema.org/Course"/>
    <owl:Class rdf:about="http://schema.org/EducationalOrganization"/>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes - Organizational Structure
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->
    
    <!-- University -->
    <owl:Class rdf:about="http://leuphana.de/ontology#University">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">University</rdfs:label>
        <rdfs:label xml:lang="de">Universität</rdfs:label>
        <rdfs:comment xml:lang="en">A higher education institution</rdfs:comment>
    </owl:Class>
    
    <!-- School (Faculty) -->
    <owl:Class rdf:about="http://leuphana.de/ontology#School">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">School</rdfs:label>
        <rdfs:label xml:lang="de">Fakultät</rdfs:label>
        <rdfs:comment xml:lang="en">A research school or faculty within the university</rdfs:comment>
    </owl:Class>
    
    <!-- Institute -->
    <owl:Class rdf:about="http://leuphana.de/ontology#Institute">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">Institute</rdfs:label>
        <rdfs:label xml:lang="de">Institut</rdfs:label>
        <rdfs:comment xml:lang="en">An institute within a school</rdfs:comment>
    </owl:Class>
    
    <!-- Research Center -->
    <owl:Class rdf:about="http://leuphana.de/ontology#ResearchCenter">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">Research Center</rdfs:label>
        <rdfs:label xml:lang="de">Forschungszentrum</rdfs:label>
        <rdfs:comment xml:lang="en">A research center focusing on specific research areas</rdfs:comment>
    </owl:Class>
    
    <!-- Research Group -->
    <owl:Class rdf:about="http://leuphana.de/ontology#ResearchGroup">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">Research Group</rdfs:label>
        <rdfs:label xml:lang="de">Forschungsgruppe</rdfs:label>
        <rdfs:comment xml:lang="en">A research group within an institute</rdfs:comment>
    </owl:Class>
    
    <!-- Department/Chair -->
    <owl:Class rdf:about="http://leuphana.de/ontology#Chair">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">Chair</rdfs:label>
        <rdfs:label xml:lang="de">Lehrstuhl</rdfs:label>
        <rdfs:comment xml:lang="en">A professorial chair or department</rdfs:comment>
    </owl:Class>
    
    <!-- Hiwi Position (Student Assistant Position) -->
    <owl:Class rdf:about="http://leuphana.de/ontology#HiwiPosition">
        <rdfs:label xml:lang="en">Hiwi Position</rdfs:label>
        <rdfs:label xml:lang="de">Hiwi-Stelle</rdfs:label>
        <skos:altLabel xml:lang="en">Student Assistant Position</skos:altLabel>
        <skos:altLabel xml:lang="de">Studentische Hilfskraft Stelle</skos:altLabel>
        <rdfs:comment xml:lang="en">A student assistant (Hiwi) position posting at the university</rdfs:comment>
    </owl:Class>
    
    <!-- Course/Module -->
    <owl:Class rdf:about="http://leuphana.de/ontology#Course">
        <rdfs:subClassOf rdf:resource="http://schema.org/Course"/>
        <rdfs:label xml:lang="en">Course</rdfs:label>
        <rdfs:label xml:lang="de">Lehrveranstaltung</rdfs:label>
        <rdfs:comment xml:lang="en">A course or module offered as part of a study program</rdfs:comment>
    </owl:Class>
    
    <!-- Module -->
    <owl:Class rdf:about="http://leuphana.de/ontology#Module">
        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#Course"/>
        <rdfs:label xml:lang="en">Module</rdfs:label>
        <rdfs:label xml:lang="de">Modul</rdfs:label>
        <rdfs:comment xml:lang="en">A module consisting of one or more courses</rdfs:comment>
    </owl:Class>
    
    <!-- Teaching Schools -->
    <owl:Class rdf:about="http://leuphana.de/ontology#TeachingSchool">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:label xml:lang="en">Teaching School</rdfs:label>
        <rdfs:comment xml:lang="en">A school for teaching (College, Graduate School, Professional School)</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://leuphana.de/ontology#College">
        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#TeachingSchool"/>
        <rdfs:label xml:lang="en">College</rdfs:label>
        <rdfs:comment xml:lang="en">Bachelor's degree programs</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://leuphana.de/ontology#GraduateSchool">
        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#TeachingSchool"/>
        <rdfs:label xml:lang="en">Graduate School</rdfs:label>
        <rdfs:comment xml:lang="en">Master's and doctoral programs</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://leuphana.de/ontology#ProfessionalSchool">
        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#TeachingSchool"/>
        <rdfs:label xml:lang="en">Professional School</rdfs:label>
        <rdfs:comment xml:lang="en">Continuing education and executive programs</rdfs:comment>
    </owl:Class>
    
