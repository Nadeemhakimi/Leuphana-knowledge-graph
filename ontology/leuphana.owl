        <rdfs:label xml:lang="en">Administrative Staff</rdfs:label>
        <rdfs:label xml:lang="de">Verwaltungspersonal</rdfs:label>
        <rdfs:comment xml:lang="en">Technical and administrative staff</rdfs:comment>
    </owl:Class>
    
    <!-- Student Assistant (HiWi) -->
    <owl:Class rdf:about="http://leuphana.de/ontology#StudentAssistant">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:label xml:lang="en">Student Assistant</rdfs:label>
        <rdfs:label xml:lang="de">Wissenschaftliche Hilfskraft</rdfs:label>
        <rdfs:comment xml:lang="en">A student assistant (HiWi)</rdfs:comment>
    </owl:Class>
    
    <!-- Dean -->
    <owl:Class rdf:about="http://leuphana.de/ontology#Dean">
        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#Professor"/>
