    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
    -->
    
    <!-- partOf - generic hierarchical relationship -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#partOf">
        <rdfs:label xml:lang="en">part of</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that something is part of something else</rdfs:comment>
        <owl:inverseOf rdf:resource="http://leuphana.de/ontology#hasPart"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#hasPart">
        <rdfs:label xml:lang="en">has part</rdfs:label>
    </owl:ObjectProperty>
    
    <!-- belongsTo - Institute belongs to School -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#belongsTo">
        <rdfs:subPropertyOf rdf:resource="http://leuphana.de/ontology#partOf"/>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
        <rdfs:domain rdf:resource="http://leuphana.de/ontology#Institute"/>
        <rdfs:range rdf:resource="http://leuphana.de/ontology#School"/>
    </owl:ObjectProperty>
    
    <!-- worksAt - Person works at Organization -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#worksAt">
        <rdfs:label xml:lang="en">works at</rdfs:label>
        <rdfs:label xml:lang="de">arbeitet bei</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <owl:inverseOf rdf:resource="http://leuphana.de/ontology#hasEmployee"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#hasEmployee">
        <rdfs:label xml:lang="en">has employee</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    </owl:ObjectProperty>
    
    <!-- memberOf - more general membership -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#memberOf">
        <rdfs:label xml:lang="en">member of</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <owl:inverseOf rdf:resource="http://leuphana.de/ontology#hasMember"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#hasMember">
        <rdfs:label xml:lang="en">has member</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    </owl:ObjectProperty>
    
    <!-- heads - Person heads Organization -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#heads">
        <rdfs:label xml:lang="en">heads</rdfs:label>
        <rdfs:label xml:lang="de">leitet</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <owl:inverseOf rdf:resource="http://leuphana.de/ontology#headedBy"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#headedBy">
        <rdfs:label xml:lang="en">headed by</rdfs:label>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    </owl:ObjectProperty>
    
    <!-- deanOf -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#deanOf">
        <rdfs:subPropertyOf rdf:resource="http://leuphana.de/ontology#heads"/>
        <rdfs:label xml:lang="en">dean of</rdfs:label>
        <rdfs:domain rdf:resource="http://leuphana.de/ontology#Dean"/>
        <rdfs:range rdf:resource="http://leuphana.de/ontology#School"/>
    </owl:ObjectProperty>
    
    <!-- teaches -->
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#teaches">
        <rdfs:label xml:lang="en">teaches</rdfs:label>
        <rdfs:label xml:lang="de">unterrichtet</rdfs:label>
        <rdfs:domain rdf:resource="http://leuphana.de/ontology#AcademicStaff"/>
        <rdfs:range rdf:resource="http://leuphana.de/ontology#Course"/>
        <owl:inverseOf rdf:resource="http://leuphana.de/ontology#taughtBy"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://leuphana.de/ontology#taughtBy">
        <rdfs:label xml:lang="en">taught by</rdfs:label>
        <rdfs:domain rdf:resource="http://leuphana.de/ontology#Course"/>
        <rdfs:range rdf:resource="http://leuphana.de/ontology#AcademicStaff"/>
    </owl:ObjectProperty>
    
