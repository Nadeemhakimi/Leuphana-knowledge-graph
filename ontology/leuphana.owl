        <rdfs:subClassOf rdf:resource="http://leuphana.de/ontology#StudyProgram"/>
        <rdfs:label xml:lang="en">Bachelor Program</rdfs:label>
        <rdfs:label xml:lang="de">Bachelor-Studiengang</rdfs:label>
    </owl:Class>
    
    <!-- Master Program -->
